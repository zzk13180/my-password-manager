{
  "name": "password-manager",
  "productName": "password-manager",
  "description": "password-manager",
  "author": {
    "name": "zhangzhankui",
    "email": "zzk13180@gmail.com"
  },
  "version": "0.0.1",
  "main": "main.js",
  "private": true,
  "keywords": [
    "Electron",
    "Angular",
    "@alyle/ui",
    "password-manager"
  ],
  "scripts": {
    "dev": "concurrently --raw \"npm run ng:serve\" \"npm run electron:serve\"",
    "build": "npm run tsc-main && ng build -c production && electron-builder build",
    "ng:serve": "ng serve",
    "electron:serve": "wait-on tcp:4200 && npm run tsc-main && npx electron . --serve",
    "tsc-main": "tsc -p tsconfig.serve.json",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "commit": "cz",
    "gh": "gh-pages -d dist"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "electron": "24.1.2",
    "electron-builder": "^23.6.0",
    "@alyle/ui": "^14.0.0",
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-devkit/build-angular": "~15.2.6",
    "@angular/animations": "~15.2.7",
    "@angular/cdk": "~15.2.7",
    "@angular/cli": "~15.2.6",
    "@angular/common": "~15.2.7",
    "@angular/compiler": "~15.2.7",
    "@angular/compiler-cli": "~15.2.7",
    "@angular/core": "~15.2.7",
    "@angular/forms": "~15.2.7",
    "@angular/language-service": "~15.2.7",
    "@angular/material": "~15.2.7",
    "@angular/platform-browser": "~15.2.7",
    "@angular/platform-browser-dynamic": "~15.2.7",
    "@angular/router": "~15.2.7",
    "@ngrx/effects": "~15.4.0",
    "@ngrx/store": "15.4.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@types/webcrypto": "^0.0.28",
    "core-js": "3.30.1",
    "rxjs": "~7.8.0",
    "ts-node": "~10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "wait-on": "7.0.1",
    "zone.js": "~0.13.0",
    "reflect-metadata": "^0.1.13",
    "cz-conventional-changelog": "^3.3.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.0.1",
    "gh-pages": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
