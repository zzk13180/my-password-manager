{
  "name": "password-manager",
  "version": "0.0.1",
  "main": "main.js",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "start": "npm-run-all -p ng:serve electron:serve",
    "start-el": "npm run electron:serve-tsc && npx electron . --serve",
    "start-dist": "npm run electron:serve-tsc && npx electron .",
    "ng": "ng",
    "ng:serve": "ng serve -c web",
    "build:prod": "npm run build -- -c production",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0 \"**/*.{cjs,vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{cjs,json,tsx,vue,html,css,scss}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --config .stylelintrc.json"
  },
  "devDependencies": {
    "@alyle/ui": "^14.0.0",
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-devkit/build-angular": "~15.2.6",
    "@angular/animations": "~15.2.7",
    "@angular/cdk": "~15.2.7",
    "@angular/cli": "~15.2.6",
    "@angular/common": "~15.2.7",
    "@angular/compiler": "~15.2.7",
    "@angular/compiler-cli": "~15.2.7",
    "@angular/core": "~15.2.7",
    "@angular/forms": "~15.2.7",
    "@angular/language-service": "~15.2.7",
    "@angular/material": "~15.2.7",
    "@angular/platform-browser": "~15.2.7",
    "@angular/platform-browser-dynamic": "~15.2.7",
    "@angular/router": "~15.2.7",
    "@ngrx/effects": "~15.4.0",
    "@ngrx/store": "15.4.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@types/webcrypto": "^0.0.28",
    "core-js": "3.30.1",
    "cross-env": "7.0.3",
    "electron": "24.1.2",
    "@electron/remote": "^2.0.9",
    "electron-builder": "23.6.0",
    "@electron/rebuild": "^3.2.12",
    "npm-run-all": "4.1.5",
    "rxjs": "~7.8.0",
    "ts-node": "~10.9.1",
    "tslib": "^2.5.0",
    "typescript": "4.8.2",
    "uuid": "^9.0.0",
    "wait-on": "7.0.1",
    "zone.js": "~0.13.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "reflect-metadata": "^0.1.13",
    "electron-store": "^8.1.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-order": "^6.0.3"
  }
}
