{
  "name": "passbox",
  "version": "1.1.0",
  "productName": "passbox",
  "description": "password manager",
  "packageManager": "pnpm@8.7.6",
  "homepage": "https://zzk13180.github.io/passbox/",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzk13180/passbox.git"
  },
  "bugs": {
    "url": "https://github.com/zzk13180/passbox/issues"
  },
  "author": {
    "name": "zhangzhankui",
    "email": "zzk13180@gmail.com"
  },
  "main": "main.js",
  "private": true,
  "keywords": [
    "Electron",
    "Angular",
    "@alyle/ui",
    "ngrx",
    "password manager",
    "passbox"
  ],
  "scripts": {
    "dev": "concurrently --raw \"npm run ng:serve\" \"npm run electron:serve\"",
    "build": "npm run tsc-main && ng build -c production && electron-builder build",
    "ng:serve": "ng serve -c development --hmr",
    "electron:serve": "wait-on tcp:4200 && npm run tsc-main && npx electron . --serve",
    "tsc-main": "tsc -p tsconfig.serve.json",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "commit": "cz",
    "gh": "gh-pages -d dist",
    "check": "tsc --noEmit -p tsconfig.json",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0 \"**/*.{cjs,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{cjs,json,tsx,html,css,scss}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --config .stylelintrc.json"
  },
  "dependencies": {
    "fs-extra": "11.1.1",
    "@electron/remote": "^2.0.11",
    "electron-context-menu": "^3.6.1",
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "@alyle/ui": "^15.0.0",
    "ngx-quill": "^22.1.0",
    "katex": "^0.16.7",
    "quill": "^1.3.7",
    "quill-emoji": "^0.2.0",
    "quill-image-resize-module": "^3.0.0",
    "@angular-builders/custom-webpack": "16.0.1",
    "@angular-devkit/build-angular": "~16.2.3",
    "@angular/cli": "~16.2.3",
    "@angular/common": "~16.2.6",
    "@angular/compiler": "~16.2.6",
    "@angular/compiler-cli": "~16.2.6",
    "@angular/core": "~16.2.6",
    "@angular/language-service": "~16.2.6",
    "@angular/platform-browser": "~16.2.6",
    "@angular/platform-browser-dynamic": "~16.2.6",
    "@angular/router": "~16.2.6",
    "@angular/animations": "^16.0.0 || ^17.0.0",
    "@angular/cdk": "~16.2.5",
    "@angular/forms": "^16.0.0 || ^17.0.0",
    "@angular/material": "~16.2.5",
    "@ngrx/effects": "~16.2.0",
    "@ngrx/store": "16.2.0",
    "@types/node": "^20.6.3",
    "@types/pako": "^2.0.0",
    "@types/uuid": "^9.0.4",
    "@types/webcrypto": "^0.0.28",
    "@types/quill": "^1.3.10",
    "@types/fs-extra": "11.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "astral-regex": "^2.0.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "core-js": "3.32.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "26.2.2",
    "electron-builder": "^24.6.4",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "fuse.js": "^6.6.2",
    "gh-pages": "^6.0.0",
    "pako": "^2.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.1",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "~10.9.1",
    "tslib": "^2.6.2",
    "typescript": "~5.1.6",
    "uuid": "^9.0.1",
    "wait-on": "7.0.1",
    "zone.js": "~0.13.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
